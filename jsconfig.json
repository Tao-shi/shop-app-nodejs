{
    "compilerOptions": {
"allowImportingTsExtensions": false,
        "checkJs":true,
        "allowJs": true,
        "module": "CommonJS",
        "moduleResolution": "node",
        "target": "ES2020",
        "jsx": "react",
        "strictNullChecks": true,
        "strictFunctionTypes": true
    },
    "exclude": [
        "node_modules",
        "**/node_modules/*"
    ]
}